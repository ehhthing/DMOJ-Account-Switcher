{
  "name": "DMOJ Account Switcher",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Simple extension to allow you to switch between multiple accounts on DMOJ.",
  "homepage_url": "https://dmoj.ca",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_title": "Switch Accounts",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "cookies",
    "https://dmoj.ca/*",
    "storage"
  ],
  "incognito": "split"
}